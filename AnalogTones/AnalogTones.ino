// Estado de cada trilha
struct trackState {
  uint16_t pos = 0;
  uint16_t nextEvent = 0;
} track;


/*
 * TODO:
 * - CONFIG ONLY OCR2B, NOT OCR2A (OCR2A does not need to be inverted)
 * - CREATE SINGLE ENTRY FOR NOTES THAT WILL BE SPLITED IN ONE FREE CHANNEL
 *   This create a single track, and notes does not need to be followed by a zero note to mute the channel.
 *   And reducing tracks reduces 32bit math required for the player;
 *   This way what needs to be implemented for single track is compensed for being just single track, getting with luck same performance.
 */
void inline play(uint16_t note) {
  if (note > 32768) {
    bitClear(note,15);
    if (chNote[0] == note) {chNote[0] = 0; return;}
    if (chNote[1] == note) {chNote[1] = 0; return;}
    if (chNote[2] == note) {chNote[2] = 0; return;}
    if (chNote[3] == note) {chNote[3] = 0; return;}
    if (chNote[4] == note) {chNote[4] = 0; return;}
    if (chNote[5] == note) {chNote[5] = 0; return;}
    if (chNote[6] == note) {chNote[6] = 0; return;}
    if (chNote[7] == note) {chNote[7] = 0; return;}
  } else {
    if (chNote[0] == 0) {chNote[0] = note; return;}
    if (chNote[1] == 0) {chNote[1] = note; return;}
    if (chNote[2] == 0) {chNote[2] = note; return;}  
    if (chNote[3] == 0) {chNote[3] = note; return;}  
    if (chNote[4] == 0) {chNote[4] = note; return;}
    if (chNote[5] == 0) {chNote[5] = note; return;}
    if (chNote[6] == 0) {chNote[6] = note; return;}  
    if (chNote[7] == 0) {chNote[7] = note; return;}  
  }
}

uint16_t * notesPointer = (uint16_t*) notes;
uint16_t * tempoPointer = (uint16_t*) tempo+1;

void inline nonBlockingPlayer(uint16_t usec) {
  if(usec-midiTimer>=bpm) {
    midiPosition++;
    midiTimer = usec;
  }

  if(midiPosition >= track.nextEvent) {
    
    play(pgm_read_word(notesPointer++));
    track.nextEvent = pgm_read_word(tempoPointer++);
    midiPosition = 0;
  }

}


void setup() {
  pinMode(0,OUTPUT);
  pinMode(3,OUTPUT);
  TCCR2A = _BV(COM2A1) | _BV(COM2A0) | _BV(COM2B1) | _BV(WGM21) | _BV(WGM20); 
  TCCR2B = _BV(CS20);
  OCR2B = 0;
}


void loop() {
  bitSet(PORTD, 0);
  uint16_t usec = micros();
  
  nonBlockingTones(usec);
  nonBlockingPlayer(usec);

 bitClear(PORTD, 0);
}
