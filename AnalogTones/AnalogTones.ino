/*
 * As notas são expressadas como metade do período, ou seja, a frequencia que o timer deve inverter um bit para gerar a frequencia da nota.
 * Isso garante uma performance maior pois menos calculos devem ser feitos.
 * apesar de um numero que é metade do período não corresponder a uma nota propriamente dita ainda pode ser usado como uma identidade.
 */

void inline nonBlockingTones(uint16_t usec) {
  if (chNote[0] && usec - chTimer[0] >= chNote[0]) {
      bit_invert(channels, 0);
      chTimer[0] = usec;
      OCR2B = decode[channels];
  }
  if (chNote[1] && usec - chTimer[1] >= chNote[1]) {
      bit_invert(channels, 1);
      chTimer[1] = usec;
      OCR2B = decode[channels];
  }
  if (chNote[2] && usec - chTimer[2] >= chNote[2]) {
      bit_invert(channels, 2);
      chTimer[2] = usec;
      OCR2B = decode[channels];
  }
  if (chNote[3] && usec - chTimer[3] >= chNote[3]) {
      bit_invert(channels, 3);
      chTimer[3] = usec;
      OCR2B = decode[channels];
  }
  if (chNote[4] && usec - chTimer[4] >= chNote[4]) {
      bit_invert(channels, 4);
      chTimer[4] = usec;
      OCR2B = decode[channels];
  }
  if (chNote[5] && usec - chTimer[5] >= chNote[5]) {
      bit_invert(channels, 5);
      chTimer[5] = usec;
      OCR2B = decode[channels];
  }
  if (chNote[6] && usec - chTimer[6] >= chNote[6]) {
      bit_invert(channels, 6);
      chTimer[6] = usec;
      OCR2B = decode[channels];
  }
  if (chNote[7] && usec - chTimer[7] >= chNote[7]) {
      bit_invert(channels, 7);
      chTimer[7] = usec;
      OCR2B = decode[channels];
  }

}

